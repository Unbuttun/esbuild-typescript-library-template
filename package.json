{
  "name": "esbuild-typescript-library-template",
  "version": "0.0.1",
  "description": " A template for building a library through typescript and bundling it through esbuild. ",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "pnpm clean && tsup-node",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unbuttun/esbuild-typescript-library-template.git"
  },
  "keywords": [
    "esbuild",
    "typescript",
    "template",
    "library"
  ],
  "author": "Unbuttun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unbuttun/esbuild-typescript-library-template/issues"
  },
  "homepage": "https://github.com/Unbuttun/esbuild-typescript-library-template#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.12",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-galex": "^2.16.7",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.4",
    "tslib": "^2.3.0",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "> 14"
  }
}
