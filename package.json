		"esbuild": "0.24.0",
		"lefthook": "^1.6.18",
		"tslib": "2.8.1",
		"tsup": "8.1.0",
		"typescript": "5.5.4",
		"vitest": "^1.6.0"
	},
	"packageManager": "pnpm@9.4.0"
